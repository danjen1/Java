<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="340.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DisplayAnimalController">
   <children>
      <VBox prefHeight="340.0" prefWidth="500.0">
         <children>
            <TableView fx:id="animalTableView" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="animalidCol" prefWidth="120.0" text="ID" />
                <TableColumn fx:id="breedCol" prefWidth="120.0" text="Breed" />
                  <TableColumn fx:id="lifespanCol" prefWidth="120.0" text="Lifespan" />
                  <TableColumn fx:id="priceCol" prefWidth="120.0" text="Price" />
              </columns>
            </TableView>
            <HBox alignment="CENTER_RIGHT" prefHeight="82.0" prefWidth="360.0" spacing="10.0">
               <children>
                  <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#onActionDisplayAnimalsDetailMenu" text="Details" />
                  <Button contentDisplay="TOP" layoutX="443.0" layoutY="37.0" mnemonicParsing="false" onAction="#onActionDisplayMainMenu" text="Back" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
